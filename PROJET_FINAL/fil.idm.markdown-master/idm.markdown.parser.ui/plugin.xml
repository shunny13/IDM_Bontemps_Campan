<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="idm.markdown.parser.ui.handlers.ParseMarkdownFileToModelInJobHandler"
            commandId="idm.markdown.parser.ui.reverseCodeCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="idm.markdown.parser.ui.toolbar"
               label="Markdown">
            <command
                  commandId="idm.markdown.parser.ui.reverseCodeCommand"
                  icon="icons/javaReverse.gif"
                  label="Markdown Parser"
                  style="push"
                  tooltip="Reverse md code into model">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="menu:org.eclipse.papyrus.ui.menu">
         <menu
               id="idm.markdown.parser.ui.menu"
               label="Java">
            <command
                  commandId="idm.markdown.parser.ui.reverseCodeCommand"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.sash.ui.folder">
         <menu
               id="idm.markdown.parser.ui.sash.menu"
               label="Java">
            <command
                  commandId="idm.markdown.parser.ui.reverseCodeCommand"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.papyrus.views.modelexplorer.modelexplorer.popup">
         <menu
               id="org.eclipse.papyrus.ui.popupmenu.modelexplorer.misc"
               label="Utils">
            <command
                  commandId="idm.markdown.parser.ui.commands.CreateAssociationFromPropertyCommand"
                  icon="icons/associationFromProperty.gif"
                  label="Create Association From Property"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.gmf.runtime.diagram.ui.DiagramEditorContextMenu">
         <menu
               id="org.eclipse.papyrus.ui.popupmenu.diagram.misc"
               label="Utils">
            <command
                  commandId="idm.markdown.parser.ui.commands.CreateAssociationFromPropertyCommand"
                  icon="icons/associationFromProperty.gif"
                  label="Create Association From Property"
                  style="push">
            </command>
            <command
                  commandId="idm.markdown.parser.ui.commands.CreateAndShowAssociationFromPropertyCommand"
                  icon="icons/associationFromProperty.gif"
                  label="Create And Show Association From Property"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any">
         <menu
               id="idm.markdown.parser.ui.javaMenu"
               label="Markdown">
            <command
                  commandId="idm.markdown.parser.ui.reverseCodeCommand"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                 <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <or>
                        <adapt
                              type="org.eclipse.core.resources.IFile">
                        </adapt>
                        <adapt
                              type="org.eclipse.core.resources.IFolder">
                        </adapt>
                     </or>
                  </iterate>
               </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            description="Parse md file into model"
            id="idm.markdown.parser.ui.reverseCodeCommand"
            name="Markdown Parser">
      </command>
   </extension>
      <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="idm.markdown.parser.ui.preference.PreferencePage"
            id="idm.markdown.parser.ui.preference.PreferencePage"
            name="Mardown Fille to Model Parser">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="idm.markdown.parser.ui.preference.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
