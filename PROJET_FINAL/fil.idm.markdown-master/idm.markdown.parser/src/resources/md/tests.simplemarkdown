    <codelines text="an example&lt;/a> inline link.&lt;/p>&#xD;&#xA;"/>
    <codelines text="&#xD;&#xA;"/>
    <codelines text="&lt;p>&lt;a href=&quot;http://example.net/&quot;>This link&lt;/a> has no&#xD;&#xA;"/>
